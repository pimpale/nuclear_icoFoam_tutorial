FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    // With friction
//    {
//        type            uniformFixedValue;
//        uniformValue    (0 0 0);
//    }

    // Walls with friction
    nozzle_expand_wall_m
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
    nozzle_fuel_wall_m
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
    nozzle_radioactive_wall_m
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
    nozzle_reaction_pre_wall_m
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }
    nozzle_reaction_wall_m
    {
        type            uniformFixedValue;
        uniformValue    (0 0 0);
    }



    // Actually useful stuff
    nozzle_inlet_m
    {
        type            uniformFixedValue;
        uniformValue    (0 -90 0);
    }
    nozzle_outlet_m
    {
        type            zeroGradient;
    }
}


