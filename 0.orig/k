FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00015;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    nozzle_expand_wall_m
    {
        type            fixedValue;
        value           $internalField;
    }
    nozzle_fuel_wall_m
    {
        type            fixedValue;
        value           $internalField;
    }
    nozzle_radioactive_wall_m
    {
        type            fixedValue;
        value           $internalField;
    }
    nozzle_reaction_pre_wall_m
    {
        type            fixedValue;
        value           $internalField;
    }
    nozzle_reaction_wall_m
    {
        type            fixedValue;
        value           $internalField;
    }

    nozzle_outlet_m
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    nozzle_inlet_m
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}

